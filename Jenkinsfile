pipeline
{
agents any
    stages
    {
        stage('Prepare')
         {
        steps
          {
            echo 'This is Prepared stage for Pipeline'
          }
       }
       stage('Build')
       {
           steps
           {
               echo 'This is Build stage'
           }
       }
       steps('Deploy')
       {
           steps
           {
               echo 'This is Deploy stagge'
           }
       }
   } 

}
